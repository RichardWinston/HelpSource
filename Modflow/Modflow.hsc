\par \cf0\b0\fs20 
\par \pard{\pntext\f2\'B7\tab}{\*\pn\pnlvlblt\pnf2\pnindent200{\pntxtb\'B7}}\fi-200\li200\cf2\strike\f1\fs24 MOC3D/GWT Options tab\cf3\strike0\{linkID=2290\}\cf0 
